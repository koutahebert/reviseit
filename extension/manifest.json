{
  "manifest_version": 3,
  "name": "ReviseIt",
  "version": "1.0",
  "description": "Make bookmarks, generate flashcards, take quizzes, and ReviseIt!",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "ReviseIt"
  },
  "icons": {
    "512": "icons/icon.png"
  }
}